<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.app.imagevideoactivity.ui.AllDataFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_display"
        android:name="com.app.imagevideoactivity.ui.TransferFragment"
        android:label="@string/title_display"
        tools:layout="@layout/fragment_display" />

    <fragment
        android:id="@+id/navigation_sign_out"
        android:name="com.app.imagevideoactivity.ui.SettingFragment"
        android:label="@string/title_sign_out"
        tools:layout="@layout/fragment_sign_out" />
</navigation>